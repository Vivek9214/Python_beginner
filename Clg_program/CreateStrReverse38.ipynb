    "# 38.\tWrite a program to create a string and split it and extract name and reverse print the string.\n",
    "str = \"Vivek Gahlot\"\n",
    "#Split\n",
    "str1 = str.split(\" \")\n",
    "print(str1)\n",
    "\n",
    "#Extract Name\n",
    "str2 = str[:6]\n",
    "print(str2)\n",
    "\n",
    "#Reverse\n",
    "str3 = str[::-1]\n",
    "print(str3)"
   ]
